pipeline{
    agent any
    stages{
        stage("git"){
            steps {
                git "https://github.com/Reddy-hub-sudo/spring-petclinic.git"

            }
        }
        stage("Maven Build Code"){
            steps {
                sh "mvn clean package"
            }
        }
        stage("Docker Build Image"){
            steps{
                sh "docker build -t vijendaraws/spring-pet:0.1 ."
            }
        }
    }
}